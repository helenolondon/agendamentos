<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <base href="./" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Agendamentos</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/fullcalendar-5.5.1/lib/main.css" />
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="~/lib/icons-1.3.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/lib/CodeSeven-toastr/build/toastr.min.css">
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert2.all.min.js">
    <link rel="stylesheet" href="~/lib/DataTables-1.10.23/datatables.min.css" />
    <link rel="stylesheet" href="~/lib/datatables-1.10.23/select.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/datatables-1.10.23/css/dataTables.bootstrap4.css" />

    @await RenderSectionAsync("styles", required: false)
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/fullcalendar-5.5.1/lib/main.js"></script>
    <script src="~/lib/jquery-ui-1.12.1.draggable/jquery-ui.js"></script>
    <script src="~/lib/moment-js-2.29.1/moment.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/CodeSeven-toastr/build/toastr.min.js"></script>
    <script src="~/lib/sweetalert/sweetalert2.all.min.js"></script>

    <script src="~/lib/datatables-1.10.23/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables-1.10.23/datatables.min.js"></script>
    <script src="~/lib/DataTables-1.10.23/dataTables.checkboxes.min.js"></script>

    <script src="~/lib/fullcalendar-5.5.1/lib/locales/pt-br.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
